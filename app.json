{
  "expo": {
    "name": "ilovedev",
    "slug": "ilovedev",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "platforms": [
      "android",
      "ios"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.apolaina.ilovedev"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.apolaina.ilovedev"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
